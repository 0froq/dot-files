{
  // ========== Visuals ==========
  // #region Visuals
  // #region Dim Unfocused
  "accessibility.dimUnfocused.enabled": true,
  "accessibility.dimUnfocused.opacity": 0.7,
  // #endregion
  // #region Font
  // "editor.fontFamily": "Monaspace Radon NF, LXGW WenKai TC",
  "editor.fontFamily": "LXGW Bright Code TC, Monaspace Argon NF",
  "editor.codeLensFontFamily": "LXGW Bright Code TC",
  // "terminal.integrated.fontFamily": "Monaspace Radon NF, LXGW WenKai TC",
  "terminal.integrated.fontFamily": "LXGW Bright Code TC",
  "editor.inlineSuggest.fontFamily": "LXGW Bright Code TC",
  "editor.fontLigatures": "'calt', 'ss01', 'ss03', 'ss04', 'ss06', 'ss07', 'ss08', 'ss10', 'liga'",
  "terminal.integrated.fontSize": 20,
  "editor.fontSize": 24,
  "editor.fontWeight": 100,
  "editor.fontVariations": "'wght' 900",
  "terminal.integrated.fontWeightBold": "400",
  "editor.cursorSmoothCaretAnimation": "on",
  // #endregion
  // #region Line Numbers
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "all",
  "editor.renderFinalNewline": "dimmed",
  // #endregion
  // #region Text Render
  "editor.renderWhitespace": "trailing",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "files.eol": "\n",
  // #endregion
  // #region Editor Element Render
  "editor.guides.bracketPairs": "active",
  // #endregion
  // #region Color Theme
  "window.autoDetectColorScheme": false,
  "workbench.preferredDarkColorTheme": "LiG Dark",
  "workbench.preferredLightColorTheme": "LiG Light",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.iconTheme": "catppuccin-frappe",
  // #endregion
  // #region Window UI
  "window.dialogStyle": "custom",
  "window.title": "${rootName}",
  "workbench.navigationControl.enabled": false,
  "workbench.tree.indent": 10,
  "files.simpleDialog.enable": true,
  "workbench.editor.highlightModifiedTabs": true,
  // #endregion
  // #region Animation Like
  "workbench.list.smoothScrolling": true,
  // #endregion
  // #endregion Visuals
  // ========== Editor ==========
  // #region Editor
  // #region Save
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.formatOnPaste": false,
  "files.insertFinalNewline": true,
  // #endregion
  // #region Search and Find
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.cursorMoveOnType": false,
  "editor.find.findOnType": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "search.exclude": {
    "**/*.snap": true,
    "**/*.svg": true,
    "**/.git": true,
    "**/.github": false,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/assets": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/public": true,
    "**/temp": true,
    "**/yarn.lock": true,
    "**/CHANGELOG*": true,
    "**/LICENSE*": true
  },
  // #endregion
  // #region Debug
  "debug.onTaskErrors": "debugAnyway",
  // #endregion
  // #region Diff
  "diffEditor.ignoreTrimWhitespace": false,
  // #endregion
  // #region Hover
  "editor.hover.sticky": true,
  // #endregion
  // #region Inline Suggestion
  "editor.inlineSuggest.enabled": true,
  // #endregion
  // #region Suggestion Widget
  "editor.suggestSelection": "first",
  // #endregion
  // #region Multi-Cursor
  "editor.multiCursorModifier": "ctrlCmd",
  // #endregion
  // #region Sticky Scroll
  "editor.stickyScroll.enabled": true,
  // #endregion
  // #region Tab
  "editor.tabSize": 2,
  // #endregion
  // #region Interactive
  "workbench.tree.expandMode": "singleClick",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.closeOnFileDelete": true,
  "window.confirmSaveUntitledWorkspace": false,
  // #endregion
  // #region Git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.untrackedChanges": "separate",
  "scm.diffDecorationsGutterWidth": 2,
  // #endregion
  // #region Problems (Diagnostics)
  "problems.sortOrder": "position",
  // #endregion
  // #region Terminal
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontWeight": "300",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  // #endregion
  // #endregion Editor
